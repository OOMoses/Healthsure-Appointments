                        "fontFamily": "Segoe UI Semibold",
                        "color": {
                            "solid": {
                                "color": "#4B3988"
                            }
                        }
                    }
                ],
                "labelText": [
                    {
                        "fontSize": 12,
                        "fontFamily": "Segoe UI",
                        "color": {
                            "solid": {
                                "color": "#433476"
                            }
                        }
                    }
                ]
            }
