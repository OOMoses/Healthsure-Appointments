{
    "name": "HealthSure Clinics Theme",
    "dataColors": [
        "#4B3988",
        "#CF3136",
        "#E37F7C",
        "#433476",
        "#F7F7F7",
        "#333333"
    ],
    "background": "#FFFFFF",
